{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./src", /* Skip type checking all .d.ts files. */
    "outDir": "./dist",
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ],
    "strictNullChecks": true,
    "strict": true
  },
  "include": [
    "./src/**/*.ts"
  ],
  "references": [
    {
      "path": "../../packages/utils"
    }
  ]
}