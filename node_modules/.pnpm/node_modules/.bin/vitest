#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/oliver/Desktop/projects/monorepo_workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@22.10.2_@vitest+ui@3.2.4/node_modules/vitest/node_modules:/home/oliver/Desktop/projects/monorepo_workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@22.10.2_@vitest+ui@3.2.4/node_modules:/home/oliver/Desktop/projects/monorepo_workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/oliver/Desktop/projects/monorepo_workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@22.10.2_@vitest+ui@3.2.4/node_modules/vitest/node_modules:/home/oliver/Desktop/projects/monorepo_workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@22.10.2_@vitest+ui@3.2.4/node_modules:/home/oliver/Desktop/projects/monorepo_workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
